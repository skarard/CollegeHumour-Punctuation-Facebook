    function replaceCH() {
    var toSinceriod = '_.';
    var replaceSinceriod ='<span class="chfb">.</span>';
    
    var toAndorpersand = 'and/or';
    var replaceAndorpersand ='<span class="chf">&</span>';
    
    var toMockwotation0 = '*"';
    var replaceMockwotation0 ='<span class="chfb">&#8220;</span>';
    
    var toMockwotation1 = '"*';
    var replaceMockwotation1 ='<span class="chfb">&#8221;</span>';
    
    var toHemiDemiSemi = ':;';
    var replaceHemiDemiSemi ='<span class="chf">;</span>';

    var toSarcastises0 = '({';
    var replaceSarcastises0 ='<span class="chf">(</span>';

    var toSarcastises1 = '})';
    var replaceSarcastises1 ='<span class="chf">)</span>';

    var toImNotAngry = '|_|';
    var replaceImNotAngry ='<span class="chf">,</span>';

    var toSuperellipsis = '...';
    var replaceSuperellipsis ='<span class="chfb">-</span>';

    var toMorganFreemark = '^[';
    var replaceMorganFreemark ='<span class="chfb">[</span>';

    var toMorganFreemark = ']^';
    var replaceMorganFreemark ='<span class="chfb">]</span>';
 
    var list, index, element, filename;
    list = document.getElementsByClassName('userContent');
    for (index = 0; index < list.length; ++index) {
        element = list[index];
        element.innerHTML.replace(toSinceriod, replaceSinceriod);
	element.innerHTML.replace(toAndorpersand, replaceAndorpersand);
	element.innerHTML.replace(toMockwotation0, replaceMockwotation0);
	element.innerHTML.replace(toMockwotation1, replaceMockwotation1);
	element.innerHTML.replace(toHemiDemiSemi, replaceHemiDemiSemi);
	element.innerHTML.replace(toSarcastises0, replaceSarcastises0);
	element.innerHTML.replace(toSarcastises1, replaceSarcastises1);
	element.innerHTML.replace(toImNotAngry, replaceImNotAngry);
	element.innerHTML.replace(toSuperellipsis, replaceSuperellipsis);
	element.innerHTML.replace(toMorganFreemark, replaceMorganFreemark);
	element.innerHTML.replace(toMorganFreemark, replaceMorganFreemark);
    }
        nodes[i].innerHTML.replace(toSinceriod, replaceSinceriod);
	nodes[i].innerHTML.replace(toAndorpersand, replaceAndorpersand);
	nodes[i].innerHTML.replace(toMockwotation0, replaceMockwotation0);
	nodes[i].innerHTML.replace(toMockwotation1, replaceMockwotation1);
	nodes[i].innerHTML.replace(toHemiDemiSemi, replaceHemiDemiSemi);
	nodes[i].innerHTML.replace(toSarcastises0, replaceSarcastises0);
	nodes[i].innerHTML.replace(toSarcastises1, replaceSarcastises1);
	nodes[i].innerHTML.replace(toImNotAngry, replaceImNotAngry);
	nodes[i].innerHTML.replace(toSuperellipsis, replaceSuperellipsis);
	nodes[i].innerHTML.replace(toMorganFreemark, replaceMorganFreemark);
	nodes[i].innerHTML.replace(toMorganFreemark, replaceMorganFreemark);
}

$("[id^='tl_unit_']").livequery(replaceCH());


